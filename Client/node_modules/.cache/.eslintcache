 useState(false);\n\n    const handleClose = () => setShow(false);\n    const handleShow = () => setShow(true);\n\n    const [watches, setWatches] = useState();\n    const [updateWatches, setUpdateWatches] = useState(false);\n\n\n\n    const [brand, setBrand] = useState();\n    const [model, setModel] = useState();\n    const [strap, setStrap] = useState();\n    const [size, setSize] = useState();\n    const [price, setPrice] = useState();\n    const [stock, setStock] = useState();\n    const [year, setYear] = useState();\n\n\n    useEffect(() => {\n        Axios.get('http://localhost:5002/api/getwatches')\n        .then(res => {\n            let watchData = res.data;\n            console.log(watchData);\n            let renderWatches = watchData.map((item) => <productcard key={item._id} brand={item.brand} price={item.price} />)\n            setWatches(renderWatches);\n            setUpdateWatches(false);\n        })\n        .catch(err => console.log(err));\n\n    }, [updateWatches])\n    \n\n\n    const getBrand = (e) => {\n        let value = e.target.value;\n        setBrand(value);\n    };\n\n    const getModel = (e) => {\n        let value = e.target.value;\n        setModel(value);\n    };\n\n    const getStrap = (e) => {\n        let value = e.target.value;\n        setStrap(value);\n    };\n\n    const getSize = (e) => {\n        let value = e.target.value;\n        setSize(value);\n    };\n\n    const getPrice = (e) => {\n        let value = e.target.value;\n        setPrice(value);\n    };\n\n    const getStock = (e) => {\n        let value = e.target.value;\n        setStock(value);\n    };\n\n    const getYear = (e) => {\n        let value = e.target.value;\n        setYear(value);\n    };\n\n    const addWatch = (e) => {\n        let details = {\n            brand: brand,\n            model: model,\n            year: year,\n            strap: strap,\n            size: size,\n            stock: stock,\n            price: price\n        }\n\n        Axios.post('http://localhost:5002/api/addwatch', details);\n        setUpdateWatches(true);\n        document.getElementById(\"brandIn\").value = \"\";\n        document.getElementById(\"modelIn\").value = \"\";\n        document.getElementById(\"yearIn\").value = \"\";\n        document.getElementById(\"strapIn\").value = \"\";\n        document.getElementById(\"sizeIn\").value = \"\";\n        document.getElementById(\"stockIn\").value = \"\";\n        document.getElementById(\"priceIn\").value = \"\";\n\n    }\n\n    return (\n        <div>\n            <Table variant='dark'  >\n                <thead className='roboto'>\n                    <tr>\n                        <th className='w-25'>Image</th>\n                        <th>Name</th>\n                        <th>Price</th>\n                        <th>QTY</th>\n                        <th>Edit</th>\n                    </tr>\n                </thead>\n                <tbody className='condensed'>\n                    <tr>\n                        <td className='ps-3'> <Image fluid src={casio} /> </td>\n                        <td>Casio A100</td>\n                        <td>R 1500</td>\n                        <td>300</td>\n                        <td> <Button variant='add' onClick={handleShow}>Update</Button> </td>\n                    </tr>\n\n                    <tr>\n                        <td> <Image fluid src={cartier} /> </td>\n                        <td>Cartier Tank</td>\n                        <td>R Price</td>\n                        <td>300</td>\n                        <td> <Button variant='add' onClick={handleShow}>Update</Button> </td>\n                    </tr>\n\n                    <tr>\n                        <td> <Image fluid src={patek} /> </td>\n                        <td>Patek Philippe</td>\n                        <td>R Price</td>\n                        <td>300</td>\n                        <td> <Button variant='add' onClick={handleShow}>Update</Button> </td>\n                    </tr>\n\n                </tbody>\n            </Table>\n\n            <Modal show={show} onHide={handleClose} size='lg' >\n                <Modal.Header closeButton>\n                    <Modal.Title>Update</Modal.Title>\n                </Modal.Header>\n\n                <Modal.Body>\n                    {/* <Form.Group className='mb-2' as={Row}>\n                        <Form.Label column>Product Name</Form.Label>\n                        <Col><Form.Control/></Col>\n                        \n                    </Form.Group>\n                    <Form.Group>\n                        <Form.Label>Product Name</Form.Label>\n                        <Form.Control/>\n                    </Form.Group> */}\n\n                    <Table>\n                        <thead>\n                            <tr>\n                                <th>Brand</th>\n                                <th>Model</th>\n                                <th>Price</th>\n                                <th>Stock</th>\n                            </tr>\n                        </thead>\n                        <tbody>\n                            <tr>\n                                <td> <Form.Control placeholder='Casio' /> </td>\n                                <td> <Form.Control placeholder='A-100WE' /> </td>\n                                <td> <Form.Control placeholder='R 1500' /> </td>\n                                <td> <Form.Control placeholder='300' /> </td>\n                            </tr>\n                        </tbody>\n                    </Table>\n\n                </Modal.Body>\n\n                <Modal.Footer>\n                    <Button onClick={handleClose}>Save Changes</Button>\n                </Modal.Footer>\n            </Modal>\n\n            <Accordion>\n                <Accordion.Item>\n                    <Accordion.Header className='header'> Add a new Watch </Accordion.Header>\n                    <Accordion.Body className=\"accordion-body\">\n                        <h1 className='pb-1' >List a new watch on the website</h1>\n                        <Form action=\"insertdoc.php\" method=\"post\">\n                            <Row>\n                                <Col className=\"mb-3 ms-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Brand </FormLabel>\n                                        <FormControl id='brandIn' onChange={getBrand} type='string' />\n                                    </FormGroup>\n                                </Col>\n\n                                <Col className=\"mb-3 me-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Model </FormLabel>\n                                        <FormControl id='modelIn' onChange={getModel} type='string' />\n                                    </FormGroup>\n                                </Col>\n                            </Row>\n                            {/* two inputs  */}\n\n                            <Row>\n                                <Col className=\"mb-3 ms-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Strap </FormLabel>\n                                        <FormControl id='strapIn' onChange={getStrap} type='string' />\n                                    </FormGroup>\n                                </Col>\n\n                                <Col className=\"mb-3 me-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Size </FormLabel>\n                                        <FormControl id='sizeIn' onChange={getSize} type='number' />\n                                    </FormGroup>\n                                </Col>\n                            </Row>\n                            {/* two inputs  */}\n\n                            <Row>\n                                <Col className=\"mb-3 ms-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Price </FormLabel>\n                                        <FormControl id='priceIn' onChange={getPrice} type='number' />\n                                    </FormGroup>\n                                </Col>\n\n                                <Col className=\"mb-3 me-auto\">\n                                    <FormGroup className='pt-2'>\n                                        <FormLabel className=' h4'> Stock </FormLabel>\n                                        <FormControl id='stockIn' onChange={getStock} type='number' />\n                                    </FormGroup>\n                                </Col>\n                            </Row>\n                            {/* two inputs  */}\n\n                            {/* <Row> */}\n                            <Col className=\"mb-3 ms-auto\">\n                                <FormGroup className='pt-2'>\n                                    <FormLabel className=' h4'> Year </FormLabel>\n                                    <FormControl id='yearIn' onChange={getYear} type='number' />\n                                </FormGroup>\n                            </Col>\n                            {/* </Row> */}\n                            {/* Input */}\n\n                            <Button variant='log' onClick={addWatch} > Submit </Button>\n                            {/* submit button */}\n                        </Form>\n                        {/* form */}\n                    </Accordion.Body>\n                    {/* accordion body  */}\n                </Accordion.Item>\n                {/* accordion item  */}\n            </Accordion>\n            {/* accordion  */}\n\n            <Col>\n                {watches}\n            </Col>\n\n\n        </div>\n    )\n}\n\nexport default Inventory",[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/pages/Products.js",["84","85","86"],[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/components/navbar.js",[],[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/components/topslider.js",["87"],[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/components/login.js",["88","89"],[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/pages/SignUp.js",[],[],"/Users/rynodebeer/Desktop/DV200/DV200_T3/term3proj/webstore/Client/src/components/productcard.js",[],[],{"ruleId":"90","severity":1,"message":"91","line":17,"column":9,"nodeType":"92","messageId":"93","endLine":17,"endColumn":13},{"ruleId":"94","severity":1,"message":"95","line":19,"column":29,"nodeType":"96","endLine":19,"endColumn":81},{"ruleId":"94","severity":1,"message":"95","line":26,"column":29,"nodeType":"96","endLine":26,"endColumn":76},{"ruleId":"94","severity":1,"message":"95","line":33,"column":29,"nodeType":"96","endLine":33,"endColumn":74},{"ruleId":"94","severity":1,"message":"95","line":43,"column":29,"nodeType":"96","endLine":43,"endColumn":81},{"ruleId":"94","severity":1,"message":"95","line":50,"column":29,"nodeType":"96","endLine":50,"endColumn":76},{"ruleId":"94","severity":1,"message":"95","line":57,"column":29,"nodeType":"96","endLine":57,"endColumn":74},{"ruleId":"90","severity":1,"message":"97","line":2,"column":32,"nodeType":"92","messageId":"93","endLine":2,"endColumn":36},{"ruleId":"90","severity":1,"message":"98","line":6,"column":8,"nodeType":"92","messageId":"93","endLine":6,"endColumn":19},{"ruleId":"90","severity":1,"message":"99","line":8,"column":8,"nodeType":"92","messageId":"93","endLine":8,"endColumn":14},{"ruleId":"90","severity":1,"message":"100","line":2,"column":31,"nodeType":"92","messageId":"93","endLine":2,"endColumn":40},{"ruleId":"90","severi